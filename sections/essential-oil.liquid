                                  height=""
                                  width=""
                                  alt="img"
                              ></a>
                              <p>{{ block.settings.link_documentation_name_sds }}</p>
                            </div>
                          {% endif %}

                          {% if block.settings.link_documentation_IFRA != blank %}
                            <div class="link_documentation">
                              <a href="{{ block.settings.link_documentation_IFRA }}">
