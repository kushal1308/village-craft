<script>
  $(document).ready(function () {
    $(document).scroll(function () {
      var w = $(window);
      var offset = $('product-info .product-form__buttons').offset();
      var btnheight = parseFloat(offset.top - w.scrollTop() - 115);
      if (btnheight <= 0) {
        $('.product_bootom_sticky').fadeIn();
        $('.product_bootom_sticky').addClass('active');
      } else {
        $('.product_bootom_sticky').fadeOut();
        $('.product_bootom_sticky').removeClass('active');
      }
    });
  });
</script>
{% assign current_variant = product.selected_or_first_available_variant %}
<div class="product_bootom_sticky bg-color-white" style="display: none;">
  <div class="page-width d-flex align-items-center">
    <div class="pro_boot_v">
      <div class="image_pro_va">
        <img src="{{ product.featured_image | img_url: 'master' }}" class="Product_image" height="" width="" alt="img" >
      </div>
      <h1 class="product-single__title font-weight-700 text-transform-uppercase color-gray font-family-Neuzeit font-size-16px line-height-21px">
        {{ product.title }}
      </h1>
      <div class="product__price">
        {% render 'price', variant: current_variant, show_vendor: section.settings.show_vendor %}
      </div>
    </div>
    <div class="quantity qv-content">
      <div class="qtydiv">
        <div class="qtybox justify-content-space-between">
          <span class="btnqty qtyminus icon icon-minus cursor-pointer">
            <svg width="12" height="3" viewBox="0 0 12 3" fill="none" xmlns="http://www.w3.org/2000/svg">
              <rect y="2.5" width="2" height="12" transform="rotate(-90 0 2.5)" fill="black"></rect>
            </svg>
          </span>
          <input
            type="text"
            id="quantity"
            name="quantity"
            value="1"
            min="1"
            class="quantity-selector quantity-input"
            readonly=""
          >
          <span class="btnqty qtyplus icon icon-plus cursor-pointer">
            <svg width="12" height="13" viewBox="0 0 12 13" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" clip-rule="evenodd" d="M7 0.5H5V5.5H0V7.5H5V12.5H7V7.5H12V5.5H7V0.5Z" fill="black"></path>
            </svg>
          </span>
        </div>
      </div>
      <form
        id="add-to-cart-form"
        method="post"
        action="/cart/add"
        data-productid=""
        accept-charset="UTF-8"
        class="form"
        enctype="multipart/form-data"
        novalidate="novalidate"
        data-type="add-to-cart-form"
      >
        <input type="hidden" name="form_type" value="product"><input type="hidden" name="utf8" value="âœ“">
        <input
          type="hidden"
          name="id"
          data-productid=""
          value="  {{ current_variant.id }}"
          value="  {{ current_variant.id }}"
        >
        <span class="variant_id" style="display: none;">{{ current_variant.id }}</span>
        <div class="product_qunatity" style="display: none;"></div>
        <div class="product-form__buttons">
          <button
            type="submit"
            name="add"
            class="font-family-Neuzeit font-weight-700 font-size-18px line-height-21px letter-spacing-0px text-transform-inherit border-radius-37px color-black border-light-black border-1px bg-light-white btn product-form__cart-submit m-0px product-form__submit_btyn  button button--full-width button--secondary {% unless current_variant.available %}sold_out{% endunless %}"
          >
            <span data-ly-mutation-count="1">
              {% unless current_variant.available %}
                {{ 'products.product.sold_out' | t }}
              {% else %}
                {{ 'products.product.add_to_cart' | t }}
              {% endunless %}
            </span>
          </button>
        </div>
      </form>
    </div>
  </div>
</div>
<section
  id="MainProduct-{{ section.id }}"
  class="section-{{ section.id }}-padding gradient color-{{ section.settings.color_scheme }}"
  data-section="{{ section.id }}"
>
